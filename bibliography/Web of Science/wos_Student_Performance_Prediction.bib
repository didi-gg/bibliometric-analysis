@article{ WOS:000739262100003,
Author = {Shreem, Salam Salameh and Turabieh, Hamza and Al Azwari, Sana and
   Baothman, Faiz},
Title = {Enhanced binary genetic algorithm as a feature selection to predict
   student performance},
