@article{ WOS:000824924700003,
Author = {Chettaoui, Neila and Atia, Ayman and Bouhlel, Med Salim},
Title = {Student Performance Prediction with Eye-Gaze Data in Embodied
   Educational Context},
Journal = {EDUCATION AND INFORMATION TECHNOLOGIES},
Year = {2023},
Volume = {28},
