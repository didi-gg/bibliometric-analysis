@article{ WOS:001033571000043,
Author = {Shen, Shuanghong and Chen, Enhong and Liu, Qi and Huang, Zhenya and
   Huang, Wei and Yin, Yu and Su, Yu and Wang, Shijin},
Title = {Monitoring Student Progress for Learning Process-Consistent Knowledge
   Tracing},
