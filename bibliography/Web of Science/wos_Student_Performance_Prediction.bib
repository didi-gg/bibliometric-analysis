@article{ WOS:000959861000001,
Author = {Apriyadi, M. Riki and Ermatita and Rini, Dian Palupi},
Title = {Hyperparameter Optimization of Support Vector Regression Algorithm using
   Metaheuristic Algorithm for Student Performance Prediction},
Journal = {INTERNATIONAL JOURNAL OF ADVANCED COMPUTER SCIENCE AND APPLICATIONS},
