@article{ WOS:000722218200009,
Author = {Baruah, Amlan Jyoti and Baruah, Siddhartha},
Title = {Data Augmentation and Deep Neuro-fuzzy Network for Student Performance
   Prediction with MapReduce Framework},
Journal = {INTERNATIONAL JOURNAL OF AUTOMATION AND COMPUTING},
