@article{ WOS:000771498800001,
Author = {Jokhan, Anjeela and Chand, Aneesh A. and Singh, Vineet and Mamun, Kabir
   A.},
Title = {Increased Digital Resource Consumption in Higher Educational
   Institutions and the Artificial Intelligence Role in Informing Decisions
   Related to Student Performance},
