@article{ WOS:000509398900090,
Author = {Tsiakmaki, Maria and Kostopoulos, Georgios and Kotsiantis, Sotiris and
   Ragos, Omiros},
Title = {Implementing AutoML in Educational Data Mining for Prediction Tasks},
Journal = {APPLIED SCIENCES-BASEL},
Year = {2020},
