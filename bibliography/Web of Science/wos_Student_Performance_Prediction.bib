Title = {Educational Data Mining for Student Performance Prediction: A Systematic
   Literature Review (2015-2021)},
Journal = {INTERNATIONAL JOURNAL OF EMERGING TECHNOLOGIES IN LEARNING},
Year = {2022},
Volume = {17},
