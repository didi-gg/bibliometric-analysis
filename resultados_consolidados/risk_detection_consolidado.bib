@article{WOS:001205448400001,
 abstract = {The postoperative 30-day mortality after the index procedure is the
third leading cause of death worldwide after heart disease and stroke
{[}24]. In elective orthopedic surgery the 30-day mortality rate is less
than 1\% {[}4]but postoperative complications can often be devastating.
