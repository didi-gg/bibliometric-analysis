 title = {Shippy Express: Augmenting accounting education with Google Sheets},
 url = {https://www.sciencedirect.com/science/article/pii/S0748575121000270},
 volume = {56},
 year = {2021}
}

@article{PAVLENKO2021666,
 abstract = {This article discusses the processes of collecting and analyzing digital footprints in LMS (Learning Management Systems). The concept of a digital footprint, the history of its appearance and examples are given. We consider the ethics of collecting these traces and the possibility of its use. A description of the tasks that need to be solved when collecting digital footprints is given, and a specific implementation in LMS "Vector" is described. This is followed by specific examples, analysis results and conclusions that have been made. At the end of the article, the main difficulties and possible ways of development and further application of this technology are described.},
