 issn = {1096-7516},
 journal = {The Internet and Higher Education},
 keywords = {Learning analytics, Latin America, Higher education, Stakeholder perspectives, Institutional adoption, Mixed methods},
 pages = {100726},
 title = {Identifying needs for learning analytics adoption in Latin American universities: A mixed-methods approach},
