 pages = {137–150},
 title = {The CASA Criteria for Evaluating Gifted and Talented Identification Systems: Cost, Alignment, Sensitivity, and Access},
 url = {https://doi-org.ez.unisabana.edu.co/10.1177/00169862221124887},
 volume = {67},
