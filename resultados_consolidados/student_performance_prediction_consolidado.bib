 issn = {2666-920X},
 journal = {Computers and Education: Artificial Intelligence},
 keywords = {Data science applications in education, Educational data mining, Improving classroom teaching, Teaching/learning strategies, Learning analytics},
 pages = {100244},
 title = {Enhancing educational evaluation through predictive student assessment modeling},
 url = {https://www.sciencedirect.com/science/article/pii/S2666920X2400047X},
 volume = {6},
 year = {2024}
}

@article{YANG202398,
 abstract = {In order to solve the shortage of the traditional educational administration system in the application of student achievement data in Colleges and universities, and on the basis of traditional data query and statistical work, this paper proposes a examination score analysis and application system based on data mining algorithm to deeply explore the internal relationship between examination scores and various factors. Through the construction of performance analysis algorithm model to predict and analyze students' performance more accurately, it is found through model comparison that the improved model has improved the accuracy of performance prediction by 1.89%, the accuracy by 1.1%, and the recall rate of the improved model algorithm by 0.5% compared with the original algorithm, which verifies the availability and value of the model.},
