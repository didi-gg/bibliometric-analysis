 title = {Exploring Pre-Service Teachers’ Cognitive Processes and Calibration with an Unsupervised Learning-Based Automated Evaluation System},
 url = {https://doi-org.ez.unisabana.edu.co/10.1177/21582440241262864},
 volume = {14},
 year = {2024}
}

